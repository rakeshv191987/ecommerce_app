<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	
   <bean id="adminBOService" class="com.bostonretailer.retailerservices.AdminBOServiceImpl" >
   
   		<!-- Commodity Service Data Access Object -->
   		<property name="commodityDao" ref="commodityDao"></property>
   		
   		<!-- Product Service Data Access Object -->
   		<property name="productDao" ref="productDao"></property>
   		
   		<!-- Order Data Access Object -->
   		<property name="orderDao" ref="orderDao"></property>
   		
   		<!-- Shipment Data Access Object -->
   		<property name="shipmentDao" ref="shipmentDao"></property>
   		
   		<!-- Vendor Data Access Object -->
   		<property name="vendorDao" ref="vendorDao"></property>
   		
   </bean>
</beans>